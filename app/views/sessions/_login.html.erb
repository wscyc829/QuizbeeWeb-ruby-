		
<div id="loginheader">
	<div id="loginpagecontainer" align="center">
		<div id="smallerlogincontainer">

			<%= form_for :session, url: login_path, remote: true do |f| %>
			<div class="heading">
				<%= f.label :username %>
			</div>
			<div>
				<%= f.text_field :username, class: 'sometextbox' %>
			</div>
			<div class="heading">
				<%= f.label :password %>
			</div>
			<div>
				<%= f.password_field :password, class: 'sometextbox' %>
			</div>
			<div>
				<%= f.submit "Log in", class: 'somebutton' %>
			</div>
			<% end %>
				<%= link_to "Sign up", signup_path ,class: 'sometext' %>
		</div>
	</div>
</div>